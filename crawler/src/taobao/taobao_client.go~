package taobao

import (
    "fmt"
    "taobao/rest"
)

GUOKU_MOBI_APP_INFO := taobao.AppInfo{"12313170", "90797bd8d5859aac971f8cc9d4e51105"}

func GetTaobaoShopInfo(nick string) *Shop {
    r = ShopGetRequest{}
    r.SetAppInfo(GUOKU_MOBI_APP_INFO.AppKey,
                 GUOKU_MOBI_APP_INFO.Secret)
    r.SetNick(nick)
    resp, _, err := r.GetResponse()
    if err != nil {
        fmt.Println(err.Error())
    }
    return resp
}
